<table>
<% var i; for (i=0; i < quizes.length; i++) { %>
   <tr>
    <td align="left"><h4><%= quizes[i].tema %>:&nbsp&nbsp</h4></td>
     <td align="left"><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
     <td align="left"><a href="quizes/<%= quizes[i].id %>/edit"><button>editar</button></a></td>
     <td>
       <form method="post" action="quizes/<%= quizes[i].id %>?_method=delete">
          <button type="submit" onClick="return confirm('Borrar: <%= quizes[i].pregunta %>');">
            borrar
          </button>
       </form>
     </td>
   </tr>
<% } %>
</table>
<a href="/quizes/new"><button type="button">Cree una nueva</button></a>
<p>O busque una que contenga...
<form method="get" action="/quizes">
  <input type="text" name="search" placeholder="Texto a buscar"/>
  <input type="submit" value="Enviar"/>
</form>
